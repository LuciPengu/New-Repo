{
  "manifest_version": 3,
  "name": "Chess.com Smart Analysis Tool",
  "version": "1.0",
  "description": "Chess analysis tool",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Get FEN"
  },
  "content_scripts": [{
    "matches": ["*://*.chess.com/*"],
    "js": ["content.js"]
  }],  "action": {
    "default_popup": "popup.html",
    "default_title": "Actions Menu"
  }
}
